<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="NamespaceMaintenanceDocument" parent="NamespaceMaintenanceDocument-parentBean">
		<property name="workflowProperties">
			<null />
		</property>
		<property name="workflowAttributes">
			<ref bean="NamespaceMaintenanceDocument-workflowAttributes" />
		</property>
	</bean>
	
	<bean id="NamespaceMaintenanceDocument-workflowAttributes" parent="WorkflowAttributes">
		<property name="routingTypeDefinitions">
			<map>
				<entry key="Namespace">
					<bean class="org.kuali.rice.krad.datadictionary.RoutingTypeDefinition">
				    	<property name="routingAttributes">
				    		<list>
				    			<bean class="org.kuali.rice.krad.datadictionary.RoutingAttribute"
				    				p:qualificationAttributeName="namespaceCode" />
				    		</list>
				    	</property>
				    	<property name="documentValuePathGroups">
				    		<list>
				    			<bean class="org.kuali.rice.krad.datadictionary.DocumentValuePathGroup">
									<property name="documentValues">
								   		<list>
								   			<value>newMaintainableObject.businessObject.code</value>
								   		</list>
							   		</property>
							   	</bean>
				    		</list>
				    	</property>					
					</bean>
				</entry>
			</map>
		</property>
		<property name="searchingTypeDefinitions">
			<list>
				<bean class="org.kuali.rice.krad.datadictionary.SearchingTypeDefinition">
					<property name="searchingAttribute">
						<bean class="org.kuali.rice.krad.datadictionary.SearchingAttribute"
							p:businessObjectClassName="org.kuali.rice.coreservice.impl.namespace.NamespaceBo"
							p:attributeName="code" />
					</property>
					<property name="documentValues">
						<list>
							<value>newMaintainableObject.businessObject.code</value>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>